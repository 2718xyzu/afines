                  RowBox[{"[", 
                   RowBox[{"t", ",", "i"}], "]"}], "]"}], "]"}], ",", 
                "\[IndentingNewLine]", 
                RowBox[{"{", 
                 RowBox[{"i", ",", 
                  RowBox[{"Length", "[", 
                   RowBox[{"rt", "[", 
                    RowBox[{"[", "t", "]"}], "]"}], "]"}]}], "}"}]}], 
               "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
              RowBox[{"Table", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"amotor", "[", 
                 RowBox[{"amt", "[", 
                  RowBox[{"[", 
                   RowBox[{"t", ",", "i"}], "]"}], "]"}], "]"}], ",", 
                "\[IndentingNewLine]", 
                RowBox[{"{", 
                 RowBox[{"i", ",", 
                  RowBox[{"Length", "[", 
                   RowBox[{"amt", "[", 
                    RowBox[{"[", "t", "]"}], "]"}], "]"}]}], "}"}]}], 
               "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
              RowBox[{"Table", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"pmotor", "[", 
                 RowBox[{"pmt", "[", 
                  RowBox[{"[", 
                   RowBox[{"t", ",", "i"}], "]"}], "]"}], "]"}], ",", 
                "\[IndentingNewLine]", 
                RowBox[{"{", 
                 RowBox[{"i", ",", 
                  RowBox[{"Length", "[", 
                   RowBox[{"pmt", "[", 
                    RowBox[{"[", "t", "]"}], "]"}], "]"}]}], "}"}]}], 
               "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}], 
            "\[IndentingNewLine]", ",", 
            RowBox[{"Frame", "\[Rule]", "True"}], ",", 
            RowBox[{"Background", "\[Rule]", "Black"}], ",", 
