       RowBox[{"frames", "=", 
        RowBox[{"Table", "\[IndentingNewLine]", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Show", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Join", "[", "\[IndentingNewLine]", 
