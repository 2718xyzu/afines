  3.632059568135697*^9, 3.632059569644183*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"sim", "[", 
     RowBox[{"d_", ",", "deltat_", ",", "genMovie_"}], "]"}], ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"dir", "=", "d"}], ",", "\[IndentingNewLine]", 
        RowBox[{"\[CapitalDelta]t", "=", "deltat"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"movieflag", "=", "genMovie"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Load", " ", "Data"}], "*)"}], "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"nrods", ",", "dt", ",", "rt"}], "}"}], "=", 
        RowBox[{"ParticleTimeSeries", "[", 
         RowBox[{"dir", ",", "\"\<rods\>\""}], "]"}]}], ";", " ", 
       "\[IndentingNewLine]", "   ", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"npms", ",", "dt", ",", "pmt"}], "}"}], "=", 
