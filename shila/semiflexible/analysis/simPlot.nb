          RowBox[{"Length", "[", "pmt", "]"}], ",", 
          RowBox[{"Length", "[", "rt", "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Generating frames for directory \>\"", "<>", "dir"}], 
        "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
